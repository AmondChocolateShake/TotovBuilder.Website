{
  "automock": false,
  "collectCoverageFrom": [
    "src/components/**/*.methods.ts",
    "src/services/**/*.ts",
    "src/utils/**/*.ts"
  ],
  "moduleFileExtensions": ["vue", "js", "json", "jsx", "ts", "tsx", "node"],
  "preset": "ts-jest",
  "setupFiles": ["jest-localstorage-mock"],
  "setupFilesAfterEnv": ["<rootDir>/src/__mocks__/servicesConfiguration.ts"],
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.vue$": "vue-jest",
    "^.+\\js$": "babel-jest"
  }
}
